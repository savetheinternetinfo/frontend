{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.18.0",
    "moment": "^2.24.0",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-feather": "^1.1.6",
    "react-helmet": "^5.2.0",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.1.8",
    "tailwindcss": "^0.7.4"
  },
  "scripts": {
    "tailwind:css": "./node_modules/.bin/tailwind build src/css/tailwind.src.css -c  tailwind.js -o src/css/tailwind.css",
    "tailwind:win": "./node_modules/.bin/tailwind.cmd build src/css/tailwind.src.css -c  tailwind.js -o src/css/tailwind.css",
    "start": "npm run tailwind:css && react-scripts start",
    "start:win": "npm run tailwind:win && react-scripts start",
    "build": "npm run tailwind:css && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
